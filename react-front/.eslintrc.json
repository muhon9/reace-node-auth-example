{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "no-unused-vars":"warn",
        "react/react-in-jsx-scope": "off",
        "react/destructuring-assignment": "off",
        "react/no-unescaped-entities": "off"
    }
}
